FromDump(~/nodeman/dumps/single_pong_packet_dump.pcap)
-> RadiotapDecap()
-> FilterPhyErr()
-> tx_filter :: FilterTX()
-> dupe :: WifiDupeFilter()
-> input_cl :: Classifier(12/0701, -); //pong, action and others

gs::GatewaySelector();



input_cl [0] -> Print(PONG) 	-> [0]gs[0] -> Discard;
input_cl [1] -> Print(OTHER)	-> [1]gs[1] -> Discard;
